    "def deco(f):\n",
    "    print(\"saludar\")\n",
    "    def wrap(s):\n",
    "        msg = f(s)\n",
    "        print(msg)\n",
    "    return wrap\n",
    "@deco\n",
    "def saludar(saludo):\n",
